<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="camera_platform"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
camera_platform
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="camera_platform">
        <sdx:configuration sdx:name="camera_platform"
                           sdx:displayName="camera_platform"
                           sdx:defaultProcessorGroup="freertos10_xilinx_domain"
                           sdx:runtimes="cpp">
            <sdx:description>camera_platform</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="camera_platform/boot/camera_platform.bif"
